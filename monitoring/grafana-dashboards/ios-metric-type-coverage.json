{
  "dashboard": {
    "id": null,
    "title": "iOS Metric Type Coverage Monitoring",
    "tags": ["health-export", "ios", "metrics", "coverage"],
    "style": "dark",
    "timezone": "UTC",
    "refresh": "5m",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "iOS Metric Type Coverage Overview",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "health_export_ios_metric_type_coverage_ratio",
            "legendFormat": "Coverage Ratio",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Unknown iOS Metric Types (Last Hour)",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "increase(health_export_ios_unknown_metric_types_total[1h])",
            "legendFormat": "{{criticality_level}} - {{ios_metric_type}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Data Loss Events (Last Hour)",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "increase(health_export_ios_metric_data_loss_total[1h])",
            "legendFormat": "{{severity}} - {{loss_reason}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "iOS Metric Type Distribution (Processing Results)",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        },
        "targets": [
          {
            "expr": "increase(health_export_ios_metric_type_distribution_total[1h])",
            "legendFormat": "{{processing_result}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "right"
          }
        }
      },
      {
        "id": 5,
        "title": "Top Unknown iOS Metric Types",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        },
        "targets": [
          {
            "expr": "topk(10, increase(health_export_ios_unknown_metric_types_total[1h]))",
            "format": "table",
            "refId": "A"
          }
        ],
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          },
          "overrides": []
        }
      },
      {
        "id": 6,
        "title": "iOS Metric Conversion Success Rate by Type",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 14
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            }
          }
        },
        "targets": [
          {
            "expr": "health_export_ios_metric_conversion_success_rate",
            "legendFormat": "{{ios_metric_type}} -> {{internal_metric_type}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "HealthKit Identifier vs Simplified Name Usage",
        "type": "bargauge",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "orientation": "horizontal",
              "thresholds": {
                "value": 80,
                "color": "red"
              }
            }
          }
        },
        "targets": [
          {
            "expr": "increase(health_export_ios_healthkit_identifier_usage_total[1h])",
            "legendFormat": "{{identifier_type}} - {{metric_category}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "Fallback Cases by Pattern",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        },
        "targets": [
          {
            "expr": "increase(health_export_ios_fallback_cases_total[5m])",
            "legendFormat": "{{fallback_type}} - {{ios_metric_pattern}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "Data Loss by Reason and Severity",
        "type": "heatmap",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 30
        },
        "targets": [
          {
            "expr": "increase(health_export_ios_metric_data_loss_total[5m])",
            "legendFormat": "{{loss_reason}} - {{severity}}",
            "refId": "A"
          }
        ],
        "options": {
          "calculate": false,
          "cellGap": 1,
          "cellValues": {},
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "spectrum",
            "reverse": false,
            "scale": "exponential",
            "scheme": "Oranges",
            "steps": 64
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": false
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "short"
          }
        }
      }
    ]
  }
}